<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingBottom="@dimen/y30">

    <RelativeLayout
        android:id="@+id/rl_item_title4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y85"
        android:gravity="center_vertical">

        <View
            android:id="@+id/nullview3"
            android:layout_width="@dimen/x4"
            android:layout_height="@dimen/y40"
            android:layout_marginRight="@dimen/x30"
            android:background="@color/app_focus" />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/x20"
            android:layout_toRightOf="@id/nullview3"
            android:text="对口率"
            android:textColor="@color/font_1"
            android:textSize="@dimen/x30" />
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_intr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl_item_title4"
        android:layout_marginBottom="@dimen/y30"
        android:layout_marginLeft="@dimen/x34"
        android:layout_marginTop="@dimen/y10"
        android:text="近五年该专业从事的前五名职业比例"
        android:textColor="@color/font_3"
        android:textSize="@dimen/x22"
        android:visibility="gone" />

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/linchart2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y334"
        android:layout_below="@id/tv_intr2" />

    <com.intention.sqtwin.widget.LableView1
        android:id="@+id/lable2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y65"
        android:layout_below="@id/tv_intr2"
        android:layout_marginLeft="@dimen/x20"
        android:layout_marginTop="@dimen/y310"
        app:lv1_textColor="@color/font_1"
        app:lv1_textsize="@dimen/x20" />

    <LinearLayout
        android:id="@+id/ll_labels1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/lable2"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:gravity="center_horizontal"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/ll_labels2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_labels1"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:gravity="center_horizontal"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/ll_sum"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_labels2"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:layout_marginTop="@dimen/y15"
        android:background="@drawable/shape_dashedline"
        android:orientation="vertical"
        android:paddingBottom="@dimen/y14"
        android:paddingLeft="@dimen/x12"
        android:paddingRight="@dimen/x12"
        android:paddingTop="@dimen/y14">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="总结："
            android:textColor="@color/font_1"
            android:textSize="@dimen/x22" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lineSpacingExtra="@dimen/y10"
            android:text=""
            android:textColor="@color/font_2"
            android:textSize="@dimen/x22" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_nulldata1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y400"
        android:layout_below="@id/rl_item_title4"
        android:background="@color/white"
        android:clickable="true"
        android:gravity="center"
        android:text="人才市场数据样本不足，无法进行大数据分析"
        android:textColor="@color/font_3"
        android:textSize="@dimen/x26"
        android:visibility="gone" />
</RelativeLayout>